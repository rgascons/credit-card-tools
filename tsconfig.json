{
  "compilerOptions": {
    "target": "es2016",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
  },
  "include": ["src/**/*.ts",],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
